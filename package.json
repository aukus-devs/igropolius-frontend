    "@types/serve-static": "^1.15.7",
    "@types/three": "^0.175.0",
    "@vitejs/plugin-react": "^4.3.4",
    "animejs": "^4.0.1",
    "cross-env": "^7.0.3",
    "eslint": "^9.21.0",
    "eslint-plugin-react-hooks": "^5.1.0",
    "eslint-plugin-react-refresh": "^0.4.19",
    "globals": "^15.15.0",
    "husky": "^9.1.7",
    "prettier": "^3.6.2",
    "react-metrika": "^0.3.3",
    "typescript": "~5.7.2",
    "typescript-eslint": "^8.24.1",
    "vite": "^6.3.3",
    "zustand": "^5.0.3"
  },
